{
  "version": 2,
  "name": "bankyapp-backend",
  "builds": [
    {
      "src": "backend/next.config.js",
      "use": "@vercel/next",
      "config": {
        "projectSettings": {
          "rootDirectory": "backend"
        }
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "backend/$1"
    }
  ]
}